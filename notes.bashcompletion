_notes()
{
    local cur prev opts
    _init_completion -s || return

    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"

    opts="--new"

    case "${cur}" in
        -*)
            COMPREPLY=($(compgen -W "${opts}" -- ${cur}))
            return 0
            ;;
    esac

    local strings="$(notes --autocomplete "${cur}")"
    COMPREPLY=( $(compgen -W "${strings}" -- "${cur}") )

    return 0
}
complete -F _notes notes
